### Initialization for substate: sub-test-state
###

###
### Elaborations for substate: sub-test-state
###

sp {elaborate*sub-test-state*state
   "Copy superstate information to sub-test-state substate"
   (state <s> ^superstate <ss>)
   (<ss> ^operator <op>)
   (<op> ^name sub-test-state)
-->
   (<s> ^name sub-test-state)
}

sp {elaborate*sub-test-state*top-state
   "Link to top-state"
   (state <s> ^superstate.top-state <ts>)
-->
   (<s> ^top-state <ts>)
}

sp {elaborate*sub-test-state*io
   "Link to io"
   (state <s> ^top-state.io <io>)
-->
   (<s> ^io <io>)
}


